<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="using:admin.ViewModels.UserManagement"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="admin.Views.UserManagement.UserListView"
             x:DataType="vm:UserListViewModel">
    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Spacing="10">
        <TextBlock Text="User List View" FontSize="24" HorizontalAlignment="Center"/>
        <Button Content="Add User" Command="{Binding NavigateToUserForm}"/>
    </StackPanel>
</UserControl>
