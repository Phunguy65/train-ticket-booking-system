<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="using:admin.ViewModels.Authentication"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="admin.Views.Authentication.LoginView"
             x:DataType="vm:LoginViewModel">
    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Spacing="10">
        <TextBlock Text="Login View" FontSize="24" HorizontalAlignment="Center"/>
        <Button Content="Go to Train List" Command="{Binding NavigateToTrainList}"/>
    </StackPanel>
</UserControl>
