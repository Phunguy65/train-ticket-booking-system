<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="using:admin.ViewModels.TrainManagement"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="admin.Views.TrainManagement.TrainListView"
             x:DataType="vm:TrainListViewModel">
    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Spacing="10">
        <TextBlock Text="Train List View" FontSize="24" HorizontalAlignment="Center"/>
        <StackPanel Orientation="Horizontal" Spacing="10" HorizontalAlignment="Center">
            <Button Content="Add Train" Command="{Binding NavigateToTrainForm}"/>
            <Button Content="User Management" Command="{Binding NavigateToUserList}"/>
            <Button Content="Audit Logs" Command="{Binding NavigateToAuditLog}"/>
        </StackPanel>
    </StackPanel>
</UserControl>
